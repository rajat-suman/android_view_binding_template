<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/treatzGraph"
    app:startDestination="@id/splash">

    <fragment
        android:id="@+id/splash"
        android:name="com.viewBindingTemplate.ui.fragments.splash.Splash"
        android:label="splash"
        tools:layout="@layout/splash">

        <action
            android:id="@+id/action_splash_to_splashFinal"
            app:destination="@id/splashFinal"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/splash"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/splashFinal"
        android:name="com.viewBindingTemplate.ui.fragments.splash.SplashFinal"
        android:label="SplashFinal"
        tools:layout="@layout/splash_final">



    </fragment>

</navigation>